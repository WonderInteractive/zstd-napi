{
  "name": "zstd-napi",
  "version": "0.0.2",
  "description": "zstd bindings with N-API",
  "repository": {
    "type": "git",
    "url": "https://github.com/drakedevel/zstd-napi.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "node-pre-gyp install --build-from-source",
    "build-prebuild": "node-pre-gyp rebuild && node-pre-gyp package",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "author": "Andrew Drake <adrake@adrake.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "node-pre-gyp-github": "^1.4.3"
  },
  "dependencies": {
    "node-addon-api": "^2.0.0",
    "node-pre-gyp": "^0.14.0"
  },
  "binary": {
    "module_name": "binding",
    "module_path": "./addon/napi-v{napi_build_version}/",
    "host": "https://github.com/drakedevel/zstd-napi/releases/download/",
    "remote_path": "v{version}",
    "package_name": "{module_name}-v{version}-napi-v{napi_build_version}-{platform}-{arch}-{libc}.tar.gz",
    "napi_versions": [
      3,
      5
    ]
  }
}
